{
  "name": "themes",
  "version": "1.0.0",
  "description": "Dependencies to build themes",
  "main": "index.js",
  "scripts": {
    "build": "cross-env SITE=$npm_config_site vite build --config config/vite.config.mjs",
    "watch": "cross-env SITE=$npm_config_site vite build --watch --config config/vite.config.mjs",
    "clean": "del-cli theme_$npm_config_site/Resources/Public/*"
  },
  "author": "Thomas Beck",
  "license": "ISC",
  "devDependencies": {
    "@fancyapps/ui": "^5.0.36",
    "@popperjs/core": "^2.11.8",
    "@spiriit/vite-plugin-svg-spritemap": "^3.0.0",
    "@stylistic/eslint-plugin": "^5.0.0",
    "bootstrap": "^5.3.6",
    "cross-env": "^7.0.3",
    "fast-glob": "^3.3.3",
    "postcss-inline-svg": "^6.0.0",
    "prettier": "^3.6.1",
    "sass": "^1.89.2",
    "sass-embedded": "^1.85.0",
    "stylelint": "^16.21.0",
    "stylelint-config-property-sort-order-smacss": "^10.0.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-prettier": "^5.0.3",
    "svgo": "^3.3.2",
    "vite": "^5.4.19",
    "vite-plugin-static-copy": "^2.3.1",
    "vite-plugin-stylelint": "^6.0.0"
  }
