{
  "name": "themes",
  "version": "1.0.0",
  "description": "Dependencies to build themes",
  "main": "index.js",
  "scripts": {
    "build": "cross-env SITE=$npm_config_site vite build --config config/vite.config.mjs",
    "watch": "cross-env SITE=$npm_config_site vite build --watch --config config/vite.config.mjs",
    "clean": "del-cli theme_$npm_config_site/Resources/Public/*"
  },
  "author": "Thomas Beck",
  "license": "ISC",
  "devDependencies": {
    "@spiriit/vite-plugin-svg-spritemap": "^3.0.0",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "cssnano": "^7.0.6",
    "del-cli": "^6.0.0",
    "rfs": "^10.0.0",
    "sass-embedded": "^1.86.3",
    "vite": "^5.0.0",
    "vite-plugin-static-copy": "^2.3.1",
    "vite-plugin-stylelint": "^6.0.0"
  }
}
