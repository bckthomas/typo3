		"thbck/theme-educcanine": "@dev",
		"thbck/theme-kwible": "@dev",
		"thbck/theme-package": "@dev",
		"thbck/theme-pimp": "@dev",
		"thbck/theme-slcreation": "@dev",
		"thbck/theme-thomasbeck": "@dev",
		"typo3/cms-backend": "^13.3",
		"typo3/cms-belog": "^13.3",
		"typo3/cms-beuser": "^13.3",
		"typo3/cms-core": "^13.3",
		"typo3/cms-dashboard": "^13.3",
		"typo3/cms-extbase": "^13.3",
