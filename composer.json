{
	"name": "typo3/cms-base-distribution",
	"description" : "TYPO3 CMS Base Distribution",
	"license": "GPL-2.0-or-later",
	"config": {
		"platform": {
			"php": "8.2"
		},
		"sort-packages": true,
		"allow-plugins": {
